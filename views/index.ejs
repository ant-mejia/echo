                  <div>
                    <span class="uk-align-center main-icon icon-target green"></span>
                    <h4 class="uk-margin-remove-top alt">You see what you want. Nothing less!</h4>
                  </div>
                  <div>
                    <span class="uk-align-center main-icon icon-chat green"></span>
                    <h4 class="uk-margin-remove-top alt">Spread your awesome thoughts with the rest of us!</h4>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    <% include partials/footer %>
