<div class="main-profile-card uk-height-large" uk-height-viewport="offset-bottom: 33">
  <div class="uk-width-large uk-align-center uk-text-center">
     <h2 class="profile-name"><%= user.username %></h2>
     <h4 class="profile-details">Joined <%= moment(user.createdAt).fromNow() %></h4>
     <h4 class="profile-details">Level <%= level %></h4>
     <h5 class="profile-initials"><%= user.firstName.split('')[0] %><%= user.lastName.split('')[0] %></h2>
  </div>
</div>
